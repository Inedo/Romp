<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>exe</OutputType>
    <TargetFramework>net452</TargetFramework>
    <LangVersion>latest</LangVersion>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <Prefer32Bit>false</Prefer32Bit>
  </PropertyGroup>
  <PropertyGroup>
    <SignAssembly>true</SignAssembly>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyOriginatorKeyFile>Romp.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="Data\Scripts\*.sql" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Inedo.Agents" Version="40.1.0" />
    <PackageReference Include="Inedo.Agents.Client" Version="528.0.0-pre0002" />
    <PackageReference Include="Inedo.ExecutionEngine" Version="64.0.0-pre0004" />
    <PackageReference Include="Inedo.SDK.DevOnly" Version="1.0.4" />
    <PackageReference Include="Inedo.UPack" Version="0.1.0-pre0026" />
    <PackageReference Include="InedoLib" Version="528.0.0-pre0002" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="System.Data.SQLite.Core" Version="1.0.108" />
    <PackageReference Include="System.ValueTuple" Version="4.4.0" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Security" />
  </ItemGroup>
</Project>